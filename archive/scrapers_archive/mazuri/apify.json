{
  "name": "mazuri-scraper",
  "version": "0.1.0",
  "buildTag": "latest",
  "env": null,
  "template": {
    "name": "python-start",
    "version": "0.1.0"
  },
  "configuration": {
    "input": {
      "title": "Mazuri Scraper Input",
      "type": "object",
      "schema": {
        "title": "Mazuri Scraper Input",
        "type": "object",
        "description": "Input for Mazuri product scraper",
        "properties": {
          "skus": {
            "title": "SKUs",
            "type": "array",
            "description": "List of SKUs to scrape from Mazuri",
            "items": {
              "type": "string"
            },
            "editor": "stringList"
          }
        },
        "required": ["skus"]
      },
      "default": {
        "skus": []
      }
    },
    "output": {
      "title": "Mazuri Scraper Output",
      "type": "object",
      "schema": {
        "title": "Mazuri Scraper Output",
        "type": "object",
        "description": "Output from Mazuri product scraper",
        "properties": {
          "SKU": {
            "title": "SKU",
            "type": "string",
            "description": "Product SKU"
          },
          "Name": {
            "title": "Name",
            "type": "string",
            "description": "Product name"
          },
          "Brand": {
            "title": "Brand",
            "type": "string",
            "description": "Product brand"
          },
          "Weight": {
            "title": "Weight",
            "type": "string",
            "description": "Product weight in pounds"
          },
          "Image URLs": {
            "title": "Image URLs",
            "type": "array",
            "description": "List of product image URLs",
            "items": {
              "type": "string"
            }
          }
        },
        "required": ["SKU", "Name", "Brand", "Weight", "Image URLs"]
      }
    }
  }
}
